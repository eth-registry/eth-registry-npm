{
  "name": "theregistry",
  "version": "1.0.0",
  "description": "Npm module for retrieving metadata from The Registry",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive --timeout 5000",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "istanbul cover _mocha -- --require babel-core/register --require babel-polyfill --recursive --timeout 5000",
    "lint": "eslint src test",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "lib",
    "src",
    "src/abi"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theregistry/theregistry.npm.git"
  },
  "keywords": [
    "npm",
    "module",
    "node",
    "javascript",
    "ethereum",
    "smartcontract"
  ],
  "author": "Alexander Mangel <ik@alexandermangel.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/theregistry/theregistry.npm/issues"
  },
  "homepage": "https://github.com/flexdinesh/npm-module-boilerplate#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdoc-md": "^1.6.0",
    "mocha": "^5.0.0",
    "prettier": "^1.14.2",
    "prettier-eslint": "^8.8.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.3.0",
    "ethjs": "^0.4.0",
    "ipfs-mini": "^1.1.2"
  }
}
